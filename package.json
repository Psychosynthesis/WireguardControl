{
  "name": "wg-control",
  "version": "1.0.0",
  "description": "Another flexable Wireguard web-interface",
  "main": "server.js",
  "license": "MIT",
  "private": true,
  "type": "module",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "flatted": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "vanicom": "^1.1.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "prettier": "3.5.3"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "style": "prettier --write '{middlewares,routes,services,utils}/**/*.js'"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 140,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "arrowParens": "avoid"
  }
}
